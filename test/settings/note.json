{
    "version": 1.0,
    "documentclass": "article",
    "options": "twocolumn",
    "package": {
        "geometry": "margin=1in",
        "inputenc": "utf8",
        "graphicx": "",
        "amsmath": "",
        "tabularx": "",
        "listings": "",
        "xcolor": "",
        "wrapfig": "",
        "seqsplit": "",
        "soul": ""
    },
    "preamble": "\\setlength{\\parindent}{0pt}\n\\lstdefinestyle{defaultstyle}{backgroundcolor=\\color{white},basicstyle=\\ttfamily\\footnotesize,breakatwhitespace=false,breaklines=true,captionpos=b,commentstyle=\\color[rgb]{0.1,0.5,0.1},keywordstyle=\\color[rgb]{0.1,0.1,0.7},stringstyle=\\color[rgb]{0.7,0.1,0.1},identifierstyle=\\color{black},numberstyle=\\tiny\\color[rgb]{0.5,0.5,0.5},stepnumber=1,numbersep=10pt,showspaces=false,showstringspaces=false,showtabs=false,tabsize=4,frame=single,rulecolor=\\color[rgb]{0.5,0.5,0.5},title=\\lstname}\n",
    "document": "\\begin{document}\n#content#\n\\end{document}",
    "config": {
        "sanitizetext": true,
        "splitconnectedlines": {
            "character": 20,
            "command": "\\seqsplit{#text#}"
        }
    },
    "rules": {
        "heading": {
            "1": "\\section{#text#}",
            "2": "\\subsection{#text#}",
            "3": "\\subsubsection{#text#}",
            "4": "\\paragraph{#text#}",
            "5": "\\subparagraph{#text#}"
        },
        "paragraph": "#text#",
        "code_block": "\\begin{lstlisting}[style=defaultstyle, language=#lang#, caption={}]\n#text#\n\\end{lstlisting}",
        "inline_math": "$#text#$",
        "block_math": "\\[\n#text#\n\\]",
        "blockquote": "\\begin{quote}\n#content#\n\\end{quote}",
        "list": {
            "ordered": "\\begin{enumerate}\n#content#\n\\end{enumerate}",
            "bullet": "\\begin{itemize}\n#content#\n\\end{itemize}"
        },
        "list_item": "\\item #content#",
        "nolabel_list_item": "\\item[] #content#",
        "image": {
            "center": "\\begin{figure}[h]\n\\centering\n\\includegraphics[width=#scale#\\textwidth]{#src#}\n\\caption{#caps#}\n\\end{figure}",
            "margin": "0.02",
            "left": "\\begin{wrapfigure}{l}{#scale#\\textwidth}\n\\centering\n\\includegraphics[width=#scale2#\\textwidth]{#src#}\n\\caption{#caps#}\n\\end{wrapfigure}",
            "right": "\\begin{wrapfigure}{r}{#scale#\\textwidth}\n\\centering\n\\includegraphics[width=#scale2#\\textwidth]{#src#}\n\\caption{#caps#}\n\\end{wrapfigure}"
        },
        "table": "\\begin{tabularx}{\\textwidth}{#columns#}\n\\hline\n#content#\n\\end{tabularx}",
        "table_row": "#content# \\\\\\hline",
        "table_cell": "#content# &",
        "citation": "\\cite{#key#}",
        "marks": {
            "strong": "\\textbf{#text#}",
            "em": "\\emph{#text#}",
            "u": "\\underline{#text#}",
            "code": "\\texttt{#text#}",
            "a": "\\href{#href#}{#text#}"
        }
    }
}